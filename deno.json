{
  "name": "@isofucius/deno-shadcn-ui",
  "version": "0.0.1",
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "src/**/*"
    ]
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react",
    "lib": [
      "dom",
      "deno.window"
    ]
  },
  "lint": {
    "exclude": [
      "ui/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 120,
    "indentWidth": 2,
    "semiColons": false,
    "singleQuote": true,
    "proseWrap": "preserve",
    "exclude": [
      "ui/"
    ]
  },
  "tasks": {
    "update": "deno run --allow-read --allow-write scripts/update.ts",
    "build-exports": "deno run --allow-read --allow-write scripts/build-exports.ts",
    "sync": "deno task update && deno task build-exports && deno fmt src && deno lint --fix src"
  },
  "imports": {
    "@radix-ui/react-accordion": "npm:@radix-ui/react-accordion@^1.2.11",
